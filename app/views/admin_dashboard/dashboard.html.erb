<div id="layoutSidenav">
  <div id="layoutSidenav_content">
    <main>
      <% flash.each do |name, msg| %>
        <%= content_tag(:div, msg, class: "alert alert-success") if msg.length > 0 %>
      <% end %>
      <div class="container-fluid">
        <h1 class="mt-4">Dashboard</h1>
        <div class="row">


          <div class="col-sm-10 offset-1">
            <section id="tabs">
              <div class="">
                <div class="row">
                  <div class="col-sm-12 ">
                    <nav>
                      <div class="nav nav-tabs nav-fill" id="nav-tab" role="tablist">
                        <a style="background: #1466FB; color: white; margin: 20px; width: 120px; border-radius: 10px;" class="nav-item nav-link" id="nav-projects-tab" data-toggle="tab" href="#projects-table" role="tab" aria-controls="nav-home" aria-selected="true">
                          <div class="card-body ">Projects</div>
                        </a>
                        <% if current_user.role.eql?("Admin") && current_user.client_company.company_name.eql?("Ambquad") %>

                          <a style="background: #FFC300; color: white; margin: 20px; width: 120px; border-radius: 10px;" class="nav-item nav-link" id="nav-users-tab" data-toggle="tab" href="#users-table" role="tab" aria-controls="nav-profile" aria-selected="false">
                            <div class="card-body">Ambquad TM Users</div>
                          </a>

                          <a style="background: #00B249; color: white; margin: 20px; width: 120px; border-radius: 10px;" class="nav-item nav-link" id="nav-companies-tab" data-toggle="tab" href="#companies-table" role="tab" aria-controls="nav-contact" aria-selected="false">
                            <div class="card-body">Client Companies</div>
                          </a>
                        <% else %>
                          <a style="background: #00B249; color: white; margin: 20px; width: 120px; border-radius: 10px;" class="nav-item nav-link" id="client_company-tab" data-toggle="tab" href="#client_company" role="tab" aria-controls="nav-contact" aria-selected="false">
                            <div class="card-body">Client Info</div>
                          </a>
                        <% end %>
                      </div>
                    </nav>
                    <div class="tab-content py-3 px-3 px-sm-0" id="nav-tabContent">
                      <div class="tab-pane fade" id="projects-table" role="tabpanel" aria-labelledby="nav-projects-tab">
                        <%= render 'shared/project_table' %>
                      </div>
                      <div class="tab-pane fade" id="users-table" role="tabpanel" aria-labelledby="nav-users-tab">
                        <%= render 'shared/user_table' %>
                      </div>
                      <div class="tab-pane fade" id="companies-table" role="tabpanel" aria-labelledby="nav-companies-tab">
                        <%= render 'shared/client_compnies_table' %>
                      </div>
                      <% unless current_user.role.eql?("Admin") %>
                        <div class="tab-pane fade" id="client_company" role="tabpanel" aria-labelledby="client_company-tab">
                          <%= render 'shared/company' %>
                        </div>
                      <% end %>
                    </div>
                  </div>
                </div>
              </div>
            </section>
          </div>

        </div>

      </div>
    </main>
  </div>
</div>

<script src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js" crossorigin="anonymous"></script>
<script src="https://cdn.datatables.net/1.10.20/js/dataTables.bootstrap4.min.js" crossorigin="anonymous"></script>
<script src="https://cdn.datatables.net/buttons/1.6.1/js/dataTables.buttons.min.js" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js" crossorigin="anonymous"></script>
<script src="https://cdn.datatables.net/buttons/1.6.1/js/buttons.html5.min.js" crossorigin="anonymous"></script>
<script src="https://cdn.datatables.net/buttons/1.6.1/js/buttons.print.min.js" crossorigin="anonymous"></script>

<script>
    $('#dataTable').DataTable();
    //$('#table').DataTable(); comment this datatable maynot load twice nit confirm
    $(document).ready(function () {
        $('#table').DataTable({
            dom: 'Bfrtip',
            buttons: [
                'excelHtml5',
                'csvHtml5',
                'pdfHtml5',
                'print'
            ]
        } );
    } );

</script>