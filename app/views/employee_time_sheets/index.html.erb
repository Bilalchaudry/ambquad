<p id="notice"><%= notice %></p>

<h1>Employee Time Sheets</h1>

<table>
  <thead>
    <tr>
      <th>Employee</th>
      <th>Employee</th>
      <th>Labour type</th>
      <th>Employee type</th>
      <th>Company</th>
      <th>Project company</th>
      <th>Manager</th>
      <th>Foreman</th>
      <th>Total hours</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_time_sheets.each do |employee_time_sheet| %>
      <tr>
        <td><%= employee_time_sheet.employee %></td>
        <td><%= employee_time_sheet.employee_id %></td>
        <td><%= employee_time_sheet.labour_type %></td>
        <td><%= employee_time_sheet.employee_type_id %></td>
        <td><%= employee_time_sheet.company %></td>
        <td><%= employee_time_sheet.project_company_id %></td>
        <td><%= employee_time_sheet.manager %></td>
        <td><%= employee_time_sheet.foreman_id %></td>
        <td><%= employee_time_sheet.total_hours %></td>
        <td><%= link_to 'Show', employee_time_sheet %></td>
        <td><%= link_to 'Edit', edit_employee_time_sheet_path(employee_time_sheet) %></td>
        <td><%= link_to 'Destroy', employee_time_sheet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employee Time Sheet', new_employee_time_sheet_path %>
