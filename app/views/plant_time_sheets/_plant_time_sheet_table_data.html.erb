<% @plant_time_sheets.each do |plant_time_sheet| %>

  <% plant_total_hours_cost_code_sum(plant_time_sheet.id) %>

  <% if plant_time_sheet.total_hours != @cost_code_and_total_hours_sum.round %>
    <tr class="table-dark" id="row<%= plant_time_sheet.id %>">
      <td><%= plant_time_sheet.plant.plant_id rescue "N/A" %></td>
      <td><%= plant_time_sheet.plant.plant_name rescue "N/A" %></td>
      <td><%= plant_time_sheet.company rescue "N/A" %></td>
      <td><%= Employee.find_by_id(plant_time_sheet.foreman.employee_id).employee_name rescue "N/A" %></td>
      <% if plant_time_sheet.submit_sheet == false || current_user.role.eql?("Admin") %>
        <td>
          <% if plant_time_sheet.total_hours > 0 %>
            <input size="4" class="mt-1 td-bg-clr td-clr-red" type="text" id="<%= plant_time_sheet.id %>" onkeypress="if (window.event.keyCode == 13) updateplantTotalHours($('#<%= plant_time_sheet.id %>').val(),$('#<%= plant_time_sheet.id %>'), <%= plant_time_sheet.id %>)"  onfocusout="updateplantTotalHours($('#<%= plant_time_sheet.id %>').val(),$('#<%= plant_time_sheet.id %>'), <%= plant_time_sheet.id %>)" value="<%= plant_time_sheet.total_hours %>">
          <% else %>
            <input size="4" class="mt-1 td-bg-clr td-clr-red" type="text" id="<%= plant_time_sheet.id %>" onkeypress="if (window.event.keyCode == 13) updateplantTotalHours($('#<%= plant_time_sheet.id %>').val(),$('#<%= plant_time_sheet.id %>'), <%= plant_time_sheet.id %>)" onfocusout="updateplantTotalHours($('#<%= plant_time_sheet.id %>').val(),$('#<%= plant_time_sheet.id %>'), <%= plant_time_sheet.id %>)" placeholder="0">
          <% end %>
        </td>
      <% else %>
        <td><%= plant_time_sheet.total_hours %></td>
      <% end %>

      <% for i in 0..4 %>
        <% if @cost_codes[i].present? %>
          <% sort_plant_cost_codes(@cost_codes) %>
          <td><%= @cost_codes[i].cost_code %>
            <% cost_code_time_sheet_id_plants(@cost_codes[i].cost_code_id, plant_time_sheet.id) %>
            <button class="cost-btn" onclick="delete_cost_code_plant(<%= @cost_code_id %>, <%= @cost_code_time_sheet_id%>,row<%= plant_time_sheet.id %>, '<%= plant_time_sheet.timesheet_created_at %>')"><%= image_tag("delete.png", alt: 'Delete Button', class: "delete-icon ") %></button>
          </td>
          <td>
            <input size="2" type="text" class="mt-1 td-bg-clr td-clr-red" id="<%= i %><%= plant_time_sheet.id %>" onkeypress="if (window.event.keyCode == 13) update_plant_cost_code_hours($('#<%= i%><%= plant_time_sheet.id %>').val(),<%= @cost_codes[i].id  %>, <%=plant_time_sheet.total_hours %>, '#<%= i%><%= plant_time_sheet.id %>')" onfocusout="update_plant_cost_code_hours($('#<%= i%><%= plant_time_sheet.id %>').val(),<%= @cost_codes[i].id  %>, <%=plant_time_sheet.total_hours %>, '#<%= i%><%= plant_time_sheet.id %>')" value="<%= @cost_codes[i].hrs %>">
          </td>
        <% elsif plant_time_sheet.total_hours > 0 %>
          <td>
            <button class="w-100 cost-bot" onclick="cost_modal_plant(<%= plant_time_sheet.plant_id %>,<%= plant_time_sheet.id %>)">+</button>
          </td>
          <td><input class="empty-cost-cost-hours-bg" readonly placeholder="0"/></td>
        <% else %>
          <td>
            <button class="w-100 cost-bot" disabled>+</button>
          </td>
          <td><input class="empty-cost-cost-hours-bg" readonly placeholder="0"/></td>
        <% end %>
      <% end %>
    </tr>
  <% else %>
    <tr id="row<%= plant_time_sheet.id %>">
      <td><%= plant_time_sheet.plant.plant_id rescue "N/A" %></td>
      <td><%= plant_time_sheet.plant.plant_name rescue "N/A" %></td>
      <td><%= plant_time_sheet.company rescue "N/A" %></td>
      <td><%= Employee.find_by_id(plant_time_sheet.foreman.employee_id).employee_name rescue "N/A" %></td>
      <td>
        <% if plant_time_sheet.total_hours > 0 %>
          <input size="4" class="mt-1 td-bg-clr" type="text" id="<%= plant_time_sheet.id %>" onkeypress="if (window.event.keyCode == 13) updateplantTotalHours($('#<%= plant_time_sheet.id %>').val(),$('#<%= plant_time_sheet.id %>'), <%= plant_time_sheet.id %>)" onfocusout="updateplantTotalHours($('#<%= plant_time_sheet.id %>').val(),$('#<%= plant_time_sheet.id %>'), <%= plant_time_sheet.id %>)"  value="<%= plant_time_sheet.total_hours %>">
        <% else %>
          <input size="4" class="mt-1 td-bg-clr" type="text" id="<%= plant_time_sheet.id %>" onkeypress="if (window.event.keyCode == 13) updateplantTotalHours($('#<%= plant_time_sheet.id %>').val(), $('#<%= plant_time_sheet.id %>'), <%= plant_time_sheet.id %>)" onfocusout="updateplantTotalHours($('#<%= plant_time_sheet.id %>').val(),$('#<%= plant_time_sheet.id %>'), <%= plant_time_sheet.id %>)"  placeholder="0">
        <% end %>
       </td>
      <% for i in 0..4 %>
        <% if @cost_codes[i].present? %>
          <% sort_plant_cost_codes(@cost_codes) %>
          <td><%= @cost_codes[i].cost_code %>
            <% cost_code_time_sheet_id_plants(@cost_codes[i].cost_code_id, plant_time_sheet.id) %>
            <button class="cost-btn" onclick="delete_cost_code_plant(<%= @cost_code_id %>, <%= @cost_code_time_sheet_id %>,row<%= plant_time_sheet.id %>, '<%= plant_time_sheet.timesheet_created_at %>')"><%= image_tag("delete.png", alt: 'Delete Button', class: "delete-icon ") %></button>
          </td>
          <td>
            <input size="2" type="text" class="mt-1 td-bg-clr" id="<%= i %><%= plant_time_sheet.id %>" onkeypress="if (window.event.keyCode == 13) update_plant_cost_code_hours($('#<%= i%><%= plant_time_sheet.id %>').val(),<%= @cost_codes[i].id  %>, <%=plant_time_sheet.total_hours %>, '#<%= i%><%= plant_time_sheet.id %>')" onfocusout="update_plant_cost_code_hours($('#<%= i%><%= plant_time_sheet.id %>').val(),<%= @cost_codes[i].id  %>, <%=plant_time_sheet.total_hours %>, '#<%= i%><%= plant_time_sheet.id %>')"  value="<%= @cost_codes[i].hrs %>">
          </td>
        <% elsif plant_time_sheet.total_hours > 0 %>
          <td>
            <button class="w-100 cost-bot" onclick="cost_modal_plant(<%= plant_time_sheet.plant_id %>,<%= plant_time_sheet.id %>)">+</button>
          </td>
          <td><input class="empty-cost-cost-hours-bg" readonly placeholder="0"/></td>
        <% else %>
          <td>
            <button class="w-100 cost-bot" disabled>+</button>
          </td>
          <td><input class="empty-cost-cost-hours-bg" readonly placeholder="0"/></td>
        <% end %>
      <% end %>
  <% end %>
  </tr>
<% end %>