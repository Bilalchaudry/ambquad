<p id="notice"><%= notice %></p>

<h1>Temporary Users</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Phone no</th>
      <th>Email</th>
      <th>Username</th>
      <th>Role</th>
      <th>Encrypted password</th>
      <th>Company</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @temporary_users.each do |temporary_user| %>
      <tr>
        <td><%= temporary_user.first_name %></td>
        <td><%= temporary_user.last_name %></td>
        <td><%= temporary_user.phone_no %></td>
        <td><%= temporary_user.email %></td>
        <td><%= temporary_user.username %></td>
        <td><%= temporary_user.role %></td>
        <td><%= temporary_user.encrypted_password %></td>
        <td><%= temporary_user.company_id %></td>
        <td><%= link_to 'Show', temporary_user %></td>
        <td><%= link_to 'Edit', edit_temporary_user_path(temporary_user) %></td>
        <td><%= link_to 'Destroy', temporary_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Temporary User', new_temporary_user_path %>
